{
  "scripts": {
    "test": "jest --silent --watchAll",
    "coverage": "jest --silent --coverage"
  },
  "devDependencies": {
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "supertest": "^6.2.2"
  },
  "editor": {
    "formatOnPaste": true,
    "formatOnSave": true
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  }
}
